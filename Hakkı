<!DOCTYPE html>
<html>
<head>
    <title>Registration Page</title>
    <style>
        #parabola {
            width: 300px;
            height: 200px;
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <h2>Registration Page</h2>
    <form id="registrationForm">
        <label for="username">Username:</label><br>
        <input type="text" id="username" name="username"><br>
        <label for="password">Password:</label><br>
        <input type="password" id="password" name="password"><br>
        <input type="submit" value="Register">
    </form>

    <h2>Registered Users:</h2>
    <div id="registeredUsers"></div>

    <h2>Parabola Graph:</h2>
    <svg id="parabola">
        <path d="M10 190 Q90 160 190 190" stroke="black" fill="transparent"/>
    </svg>

    <script>
        document.getElementById('registrationForm').addEventListener('submit', function(event) {
            event.preventDefault();
            var username = document.getElementById('username').value;
            var password = document.getElementById('password').value;
            var registeredUsersDiv = document.getElementById('registeredUsers');
            registeredUsersDiv.innerHTML += 'Username: ' + username + ', Password: ' + password + '<br>';
        });
    </script>
</body>
</html>
